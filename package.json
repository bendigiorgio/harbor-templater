{
  "name": "harbor-templater",
  "description": "A new CLI generated with oclif",
  "version": "0.0.0",
  "author": "Ben",
  "bin": {
    "harbor-templater": "./bin/run.js"
  },
  "bugs": "https://github.com/harbor-templater/harbor-templater/issues",
  "dependencies": {
    "@inquirer/prompts": "^8.2.0",
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "picomatch": "^4.0.3",
    "tar": "^7.5.3"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@commitlint/cli": "^20.3.1",
    "@commitlint/config-conventional": "^20.3.1",
    "@eslint/compat": "^1",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/exec": "^7.1.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^12.0.2",
    "@semantic-release/npm": "^13.1.3",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@types/chai": "^4",
    "@types/mocha": "^10",
    "@types/node": "^24.10.9",
    "@types/picomatch": "^4.0.2",
    "chai": "^4",
    "eslint": "^9",
    "eslint-config-oclif": "^6",
    "eslint-config-prettier": "^10",
    "husky": "^9.1.7",
    "mocha": "^10",
    "oclif": "^4",
    "semantic-release": "^25.0.2",
    "shx": "^0.3.3",
    "ts-node": "^10",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "./bin",
    "./dist",
    "./oclif.manifest.json"
  ],
  "homepage": "https://github.com/harbor-templater/harbor-templater",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "oclif": {
    "bin": "harbor-templater",
    "dirname": "harbor-templater",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " "
  },
  "repository": "harbor-templater/harbor-templater",
  "scripts": {
    "build": "shx rm -rf dist && shx rm -f tsconfig.tsbuildinfo && tsc -p tsconfig.json",
    "format": "biome format",
    "lint": "biome lint",
    "check": "biome check",
    "lint:fix": "biome lint --fix",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "pnpm run lint",
    "prepack": "pnpm run build && oclif manifest && oclif readme",
    "prepublishOnly": "pnpm test && pnpm run build && pnpm run prepack",
    "prepare": "husky",
    "commitlint": "commitlint --edit",
    "release": "semantic-release",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts",
  "packageManager": "pnpm@10.12.1+sha512.f0dda8580f0ee9481c5c79a1d927b9164f2c478e90992ad268bbb2465a736984391d6333d2c327913578b2804af33474ca554ba29c04a8b13060a717675ae3ac"
}
